{
  "name": "chunhaofen-homepage",
  "version": "1.0.0",
  "description": "Personal homepage showcasing electron-infra-kit projects",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "type-check": "tsc --noEmit",
    "validate-projects": "tsx docs/.vitepress/data/validate-projects.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "check-links": "linkinator dist --config linkinator.config.json",
    "analyze-bundle": "npm run build && du -sh dist && du -sh dist/**/*"
  },
  "keywords": [
    "vitepress",
    "electron",
    "portfolio"
  ],
  "author": "chunhaofen",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "linkinator": "^7.5.1",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "vitest": "^4.0.16",
    "vue": "^3.4.0"
  }
}
